#!/bin/sh
# Sonarr s6 service (pkg version)

# Required environment for .NET on FreeBSD
export HOME=/config
export XDG_CONFIG_HOME=/config/.config
export XDG_DATA_HOME=/config/.local/share
export LD_LIBRARY_PATH=/usr/local/lib
export CLR_OPENSSL_VERSION_OVERRIDE=35
export DOTNET_SYSTEM_NET_DISABLEIPV6=1

cd /config
exec /usr/local/bin/s6-setuidgid bsd /usr/local/share/sonarr/bin/Sonarr -nobrowser -data=/config
